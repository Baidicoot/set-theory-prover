Sort "Set"

Const("elem","Set -> Set -> Prop")
Notation("a ∈ b","elem a b")

Const("empty","Set")
Axiom("empty_set","forall x, ¬(x ∈ empty)")

Const("union","Set -> Set -> Set")
Axiom("union_prop","forall A B x, x ∈ union A B => (x ∈ a) or (x ∈ b)")

Axiom("eq_set","forall x y, x = y <=> forall z, z ∈ x <=> z ∈ y")

theorem("union_empty_eq","forall x, union x empty = x")
    intros"x"
    refine "eq_set[union x empty][x] _"
    intros "z"